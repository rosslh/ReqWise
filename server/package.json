{
  "name": "reqwise-server",
  "version": "1.0.0",
  "description": "A beautiful fastify app",
  "main": "server.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "test": "tap test/**/*.test.js",
    "start": "fastify start -l info -p $PORT -a 0.0.0.0 src/index.js --body-limit 2097152 --trustProxy true",
    "dev": "fastify start -l info -P -w src/index.js --port 3001 --body-limit 2097152 --trustProxy true",
    "sql": "cloud_sql_proxy_x64.exe -instances=reqwise:us-central1:reqwise=tcp:1111",
    "lint": "eslint src/**/*.js",
    "licenseChecker": "license-checker --onlyAllow \"MIT;BSD;Apache-2.0;BSD-3-Clause;ISC;BSD-2-Clause;WTFPL;CC-BY-3.0;CC-BY-4.0;CC0-1.0;Unlicense\" --excludePackages \"reqwise-client@1.0.0;reqwise-server@1.0.0;shimport@1.0.1;svelte-select@3.7.0;buffers@0.1.1;event-stream@0.9.8;png-js@0.1.1\""
  },
  "engines": {
    "node": "12.x"
  },
  "keywords": [
    "fastify",
    "nodejs"
  ],
  "author": "Ross Hill <ross@rosshill.ca>",
  "dependencies": {
    "@google-cloud/storage": "^5.1.2",
    "@slack/web-api": "^5.10.0",
    "bcrypt": "^3.0.7",
    "dotenv": "^8.2.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.0.2",
    "fastify-cli": "^2.0.2",
    "fastify-cors": "^4.0.0",
    "fastify-formbody": "^4.0.3",
    "fastify-jwt": "^2.1.2",
    "fastify-plugin": "^2.0.0",
    "fuse.js": "^6.4.0",
    "generate-avatar": "^1.4.4",
    "hashids": "^2.2.1",
    "html-to-mrkdwn": "^3.0.0",
    "html-to-text": "^5.1.1",
    "is-docker": "^2.0.0",
    "knex": "^0.20.4",
    "make-promises-safe": "^5.0.0",
    "nodemailer": "^6.4.6",
    "pg": "^7.15.0",
    "pino-colada": "^1.4.4",
    "pump": "^3.0.0",
    "quill-delta-to-html": "^0.12.0",
    "request-promise": "^4.2.5",
    "resolve-from": "^5.0.0",
    "sharp": "^0.25.4",
    "socket.io": "^2.3.0",
    "uuid": "^8.1.0",
    "yargs-parser": "^16.0.0"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "license-checker": "^25.0.1",
    "pre-commit": "^1.2.2",
    "standard": "^14.0.0",
    "tap": "^14.8.2"
  }
}
