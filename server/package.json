{
  "name": "reqwise-server",
  "version": "1.0.0",
  "description": "A beautiful fastify app",
  "main": "server.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "fastify start -l info -p $PORT -a 0.0.0.0 src/index.js --body-limit 2097152 --options",
    "dev": "fastify start -l info -P -w src/index.js --port 3001 --body-limit 2097152 --options",
    "sql": "cloud_sql_proxy_x64.exe -instances=reqwise:us-central1:reqwise-qa-1=tcp:1112",
    "sqlProduction": "cloud_sql_proxy_x64.exe -instances=reqwise:us-central1:reqwise-qa-1=tcp:1112,reqwise:us-central1:reqwise=tcp:1111",
    "lint": "eslint src/**/*.js --fix",
    "licenseChecker": "license-checker",
    "seed": "knex migrate:latest --env=dev && knex seed:run --env=dev",
    "//": "NOTE: cloc is GPL-2.0 and must be installed locally",
    "count-loc": "cloc --vcs=git --exclude-dir=migrations,test --not-match-f=package-lock\\.json ."
  },
  "engines": {
    "node": "12.x"
  },
  "keywords": [
    "fastify",
    "nodejs"
  ],
  "dependencies": {
    "@google-cloud/storage": "^5.2.0",
    "@slack/web-api": "^5.11.0",
    "async-retry": "^1.3.1",
    "atob": "^2.1.2",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.2.1",
    "fastify-autoload": "^3.0.8",
    "fastify-cli": "^2.2.0",
    "fastify-cors": "^4.1.0",
    "fastify-formbody": "^4.0.3",
    "fastify-jwt": "^2.1.3",
    "fastify-plugin": "^2.3.2",
    "fuse.js": "^6.4.1",
    "generate-avatar": "^1.4.4",
    "hashids": "^2.2.1",
    "html-to-mrkdwn": "^3.0.0",
    "html-to-text": "^5.1.1",
    "knex": "^0.21.5",
    "make-promises-safe": "^5.0.0",
    "nodemailer": "^6.4.11",
    "pg": "^8.3.2",
    "quill-delta-to-html": "^0.12.0",
    "request-promise": "^4.2.6",
    "resolve-from": "^5.0.0",
    "sharp": "^0.25.4",
    "socket.io": "^2.3.0",
    "uuid": "^8.3.0",
    "yargs-parser": "^19.0.1"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-plugin-editorconfig": "^2.1.3",
    "license-checker": "^25.0.1"
  }
}
