{
  "name": "reqwise-client",
  "version": "1.0.0",
  "private": true,
  "description": "A beautiful sapper app",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run gen-blog && sapper build --legacy",
    "clean": "rm -rf __sapper__/ && rm -f session-store.db",
    "//": "NOTE: cloc is GPL-2.0 and must be installed locally",
    "count-loc": "cloc --vcs=git --exclude-dir=static,.firebase --not-match-f=package-lock\\.json .",
    "deploy": "firebase deploy --only functions",
    "dev": "sapper dev",
    "export": "sapper export --legacy",
    "fb_start": "npm run shell",
    "gen-blog": "cd src/blog && npx m2j . --content -o ../routes/blog/data.json",
    "license-checker": "license-checker --onlyunknown",
    "lint": "eslint src/**/*.svelte src/**/*.js",
    "logs": "firebase functions:log",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "node __sapper__/build",
    "test": "cypress run",
    "test-gui": "cypress open"
  },
  "lint-staged": {
    "**/*.js": "eslint",
    "**/*.svelte": "eslint"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^1.0.6",
    "@google-cloud/firestore": "^4.2.0",
    "@shopify/draggable": "^1.0.0-beta.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "connect-loki": "^1.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "intro.js": "^2.9.3",
    "node-fetch": "^2.6.1",
    "quill": "^1.3.7",
    "sapper-environment": "^1.0.1",
    "showdown": "^1.9.1",
    "sirv": "^1.0.6",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "date-fns": "^2.16.1",
    "eslint": "^7.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "file-saver": "^2.0.2",
    "fuse.js": "^6.4.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.3.0",
    "lodash": "^4.17.20",
    "markdown-to-json": "^0.5.4",
    "normalize-url": "^5.1.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "postcss": "^8.0.8",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "query-string": "^6.13.1",
    "rollup": "^2.28.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.9",
    "sass": "^1.26.10",
    "slugify": "^1.4.5",
    "svelte": "^3.28.0",
    "svelte-icons": "^2.1.0",
    "svelte-media": "0.0.3",
    "svelte-preprocess": "^4.3.0",
    "svelte-select": "^3.11.1",
    "sveltejs-tippy": "^3.0.0",
    "text-diff": "^1.0.1"
  },
  "optionalDependencies": {
    "cypress": "^5.1.0",
    "cypress-wait-until": "^1.7.1"
  },
  "engines": {
    "node": "12"
  }
}
