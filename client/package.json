{
  "name": "reqwise-client",
  "description": "A beautiful svelte app",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "sapper build --legacy",
    "deploy": "firebase deploy --only functions",
    "dev": "sapper dev",
    "export": "sapper export --legacy",
    "fb_start": "npm run shell",
    "logs": "firebase functions:log",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "run:client": "npm run dev",
    "run:backend": "cd ../server && npm run dev",
    "run:fullStack": "run-p --race run:backend run:client",
    "test": "server-test run:fullStack \"http-get://localhost:3000|http-get://localhost:3001\" cy:run",
    "testGui": "server-test run:fullStack \"http-get://localhost:3000|http-get://localhost:3001\" cy:open",
    "lint": "eslint src/**/*.svelte src/**/*.js",
    "clean": "rm -rf __sapper__/ && rm -f session-store.db",
    "licenseChecker": "license-checker --onlyAllow \"MIT;BSD;Apache-2.0;BSD-3-Clause;ISC;BSD-2-Clause;WTFPL;CC-BY-3.0;CC-BY-4.0;CC0-1.0;Unlicense\" --excludePackages \"reqwise-client@1.0.0;reqwise-server@1.0.0;shimport@1.0.1;svelte-select@3.10.1;buffers@0.1.1;event-stream@0.9.8;png-js@0.1.1\""
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^1.0.6",
    "@google-cloud/firestore": "^3.8.6",
    "@shopify/draggable": "^1.0.0-beta.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "connect-loki": "^1.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.9.0",
    "node-fetch": "^2.6.0",
    "quill": "^1.3.7",
    "sapper-environment": "^1.0.1",
    "sirv": "^0.4.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "date-fns": "^2.12.0",
    "eslint": "^7.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "file-saver": "^2.0.2",
    "firebase-functions-test": "^0.2.0",
    "fuse.js": "^6.0.4",
    "license-checker": "^25.0.1",
    "lodash.range": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "postcss": "^7.0.27",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.28.0",
    "sass": "^1.26.10",
    "svelte": "^3.24.1",
    "svelte-icons": "^2.1.0",
    "svelte-media": "0.0.3",
    "svelte-preprocess": "^4.0.9",
    "svelte-preprocess-postcss": "^1.1.1",
    "svelte-select": "^3.10.1",
    "sveltejs-tippy": "^3.0.0",
    "text-diff": "^1.0.1"
  },
  "optionalDependencies": {
    "cypress": "^4.12.1",
    "cypress-plugin-retries": "^1.5.2",
    "cypress-wait-until": "^1.7.1",
    "start-server-and-test": "^1.11.3"
  }
}